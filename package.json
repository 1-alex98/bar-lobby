{
  "name": "bar-lobby",
  "private": true,
  "description": "Lobby client for the RTS game Beyond All Reason",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jazcash/bar-lobby.git"
  },
  "author": "Jazcash",
  "license": "gpl-3.0",
  "scripts": {
    "build": "vue-cli-service electron:build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "dump-webpack-config": "vue inspect > output.js"
  },
  "main": "background.js",
  "dependencies": {
    "@iconify/json": "^1.1.420",
    "@sinclair/typebox": "^0.20.5",
    "@types/electron-devtools-installer": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vueuse/sound": "^1.1.7",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-store": "^8.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "greenworks": "^0.1.0",
    "jaz-ts-utils": "^1.2.0",
    "lint-staged": "^9.5.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "tachyon-client": "github:Jazcash/tachyon-client",
    "typescript": "^4.4.4",
    "vue": "^3.0.0",
    "vue-auto-routing": "^1.0.0",
    "vue-cli-plugin-auto-routing": "~1.3.0",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-import-components": "~1.2.1",
    "vue-router": "^4.0.12",
    "vue-router-layout": "^0.4.0",
    "vuex": "^4.0.0-0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
